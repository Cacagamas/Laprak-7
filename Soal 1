def cek_anagram(kata1, kata2):
    # Menghapus spasi dan mengubah huruf menjadi huruf kecil agar tidak peka terhadap kapitalisasi
    kata1 = kata1.replace(" ", "").lower()
    kata2 = kata2.replace(" ", "").lower()
    
    # Menghitung panjang kedua kata
    panjang_kata1 = len(kata1)
    panjang_kata2 = len(kata2)
    
    # Jika panjang kedua kata tidak sama, maka mereka bukan anagram
    if panjang_kata1 != panjang_kata2:
        return False
    
    # Mengonversi kata kedua menjadi list karakter untuk mempermudah manipulasi
    list_kata2 = list(kata2)
    
    # Memeriksa setiap karakter dalam kata pertama
    for karakter in kata1:
        # Jika karakter tersebut ada dalam list kata kedua, hapus karakter tersebut dari list
        if karakter in list_kata2:
            list_kata2.remove(karakter)
        # Jika karakter tidak ada dalam list kata kedua, kata-kata bukan anagram
        else:
            return False
    
    # Jika list kata kedua kosong setelah iterasi, maka kata-kata adalah anagram satu sama lain
    if len(list_kata2) == 0:
        return True
    else:
        return False

# Contoh penggunaan
kata1 = input("Masukkan kata pertama: ")
kata2 = input("Masukkan kata kedua: ")

if cek_anagram(kata1, kata2):
    print("Kedua kata adalah anagram satu sama lain.")
else:
    print("Kedua kata bukan anagram.")
